<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/market_color_white"/>
        </shape>
    </item>

    <item
        android:left="-2dp"
        android:right="-2dp"
        android:top="-2dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <stroke android:color="@color/market_color_primary" android:width="1dp"/>
        </shape>
    </item>
</layer-list>